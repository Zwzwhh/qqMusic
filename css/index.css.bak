.newSongs{
	height:500px;
	margin-top:60px;
	background: linear-gradient(#F3F3F3, #FFFFFF);
	position:relative;
}
.newSongs-head{
/*	position: relative;*/
	margin:0 auto;
	text-align:center;
    padding-top: 3%;
    padding-bottom: 3%;
	background:url("../img/index_tit.png") no-repeat;
	background-position:50% 11.5%;
	overflow:hidden;
}
.newSongs-nav li{
	float:left;
	margin-right: 50px;
	height:50px;
	line-height:50px;
}
.all-play{
	display: block;
    width: 123px;
    height: 38px;
    line-height: 38px;
    text-align: center;
    border-radius: 3px;
}
.tab_right{
	margin-left: 250px;
}
.firstLi{
	width:33.3%;
	height:92px;
	margin-top:20px;
	position:relative;
}
.firstLi a img{
	position:absolute;
	top:0px;
	left:0px;
}
.firstLi b a img{
	position:absolute;
	border-radius:50%;
	top:30px;
	left:30px;
}
.firstLi h4 a {
	width:180px;
	text-overflow:ellipsis;
	white-space:nowrap;
	overflow:hidden;
	position:absolute;
	color:#8F8F8F;
	top:30px;
	left:100px;
}
.firstLi p a {
	position:absolute;
	top:50px;
	color:#B0B0B0;
	left:100px;
}
.firstLi .song_list_time{
	position:absolute;
	top:40px;
	color:#B0B0B0;
	left:330px;
}
