/*赥Ƽtab*/
.index-nav{
	text-align:center;
}
.index_tab{
	height: 50px;
	line-height: 50px;
	display:inline-block;
	text-align:center;
}
.index_tab>li{
	float:left;
	margin-right: 47px;
	height:50px;
	line-height:50px;
}
.mod-play{
	display:inline-block;
	margin:0px;
	padding:0px;
	text-align:center;
}
.mod-play ul>li{
	width:220px;
	height:300px;
	float:left;
	line-height:220px;
	margin-right:6px;
	position:relative;
/*	border:1px solid red;*/
}
.mod-play ul>li dt a{
	width:220px;
	height:220px;
	display:block;
}
.mod-play ul>li dt a img{
	width:220px;
	height:220px;
	position: absolute;
	background-size:100%;
    top: 0;
    left: 0;
}
.mod-play ul>li dt b{
    width: 70px;
    height: 70px;
    background: #F2F2F2;
	border-radius:50%;
    position: absolute;
    top: 80px;
    left: 80px;
}
.mod-play ul>li dt b img{
	width:30px;
	height:30px;
	position: absolute;
    top: 20px;
    left:20px;
}
.mod-play dd:nth-child(2){
	color:#ddd;
	position: absolute;
    top: 128px;
    left:0px;
}
.mod-play dd:nth-child(3) a{
	color:#999999;
	text-decoration:none;
	position: absolute;
    top: 150px;
    left:0px;
}
